{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.OpenTelemetry", "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "OpenTelemetry",
        "Args": {
          "Endpoint": "http://seq:5341/ingest/otlp/v1/logs",
          "Protocol": "HttpProtobuf"
        }
      }
    ]
  }
}
